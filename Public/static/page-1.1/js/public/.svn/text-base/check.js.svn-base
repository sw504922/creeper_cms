// JavaScript Document
$(function(){
	$("form .input_t").click(function(){
		$(this).val("");
		});
	$("form .input_t").blur(function(){
		var inval = $(this).attr("value");
		if(inval == ""){
		$(this).attr("value","请输入您的OA账号");
		}
		});
	 $(".input_password").click(function(){
		$(this).hide();
		$(".input_p").css("display","block");
		$(".input_p").focus();
		});
	
})
	